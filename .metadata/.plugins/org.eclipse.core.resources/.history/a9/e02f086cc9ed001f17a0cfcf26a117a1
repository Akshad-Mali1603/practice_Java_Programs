import java.util.Scanner;

public class mainInputOutputStream {

	public static void main(String[] args) {

		TestInputStream in = new TestInputStream();
		TestOutputstream out = new TestOutputstream();
		
		Scanner sc = new Scanner(System.in);
		
		while (true) {
			System.out.println("\n Insert data  [1]"
							 + "\n Display data [2]"
							 + "\n Update data  [3]"
							 + "\n Delete data  [4]"
							 + "\nEnter your choice: ");
			int ch= sc.nextInt();
			
			switch (ch) {
			case 1:
				out.Output();
				break;
			
			case 2:
				
			default:
				break;
			}
		}

	}

}
